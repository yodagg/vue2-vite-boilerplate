{
    "include": ["."],
    "exclude": ["**/*.spec.ts"],
    "compilerOptions": {
        "outDir": "dist",
        "target": "ES2020",
        "module": "ES2020",
        "moduleResolution": "node",
        "strict": true,
        "declaration": true,
        "sourceMap": true,
        "noImplicitOverride": true,
        "noUnusedLocals": true,
        "esModuleInterop": true,
        "baseUrl": ".",
        "jsx": "preserve"
    },
    "vueCompilerOptions": {
        "target": 2.7
    }
}
